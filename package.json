{
  "name": "custom-tunnel",
  "displayName": "Custom Tunnel",
  "description": "로컬 개발 서버를 공개 URL로 공유하고, 실시간 콘솔 모니터링과 QR 코드 생성으로 쉽게 테스트하세요",
  "version": "1.0.0",
  "publisher": "dbckdgjs369",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbckdgjs369/debug_tool.git"
  },
  "homepage": "https://github.com/dbckdgjs369/debug_tool#readme",
  "bugs": {
    "url": "https://github.com/dbckdgjs369/debug_tool/issues"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Development Tools"
  ],
  "keywords": [
    "tunnel",
    "localhost",
    "share",
    "debug",
    "mobile",
    "console",
    "remote"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tunnel-explorer",
          "title": "Custom Tunnel",
          "icon": "$(globe)"
        }
      ]
    },
    "views": {
      "tunnel-explorer": [
        {
          "type": "webview",
          "id": "tunnelView",
          "name": "Tunnel Dashboard"
        }
      ]
    },
    "commands": [
      {
        "command": "tunnel.start",
        "title": "Start Tunnel",
        "category": "Tunnel",
        "icon": "$(play)"
      },
      {
        "command": "tunnel.stop",
        "title": "Stop Tunnel",
        "category": "Tunnel",
        "icon": "$(stop)"
      },
      {
        "command": "tunnel.refresh",
        "title": "Refresh",
        "category": "Tunnel",
        "icon": "$(refresh)"
      },
      {
        "command": "tunnel.copyUrl",
        "title": "Copy URL to Clipboard",
        "category": "Tunnel",
        "icon": "$(clippy)"
      },
      {
        "command": "tunnel.openUrl",
        "title": "Open URL in Browser",
        "category": "Tunnel",
        "icon": "$(link-external)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "precompile": "node scripts/build-config.js",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "npm run compile && yes | npx @vscode/vsce package --allow-missing-repository",
    "package:dev": "TUNNEL_ENV=development npm run compile && yes | npx @vscode/vsce package --allow-missing-repository --out custom-tunnel-dev.vsix"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^8.0.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "ws": "^8.14.2"
  }
}
